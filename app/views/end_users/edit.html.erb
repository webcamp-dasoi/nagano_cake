<h2>登録情報編集</h2>

<%= form_with　model: @end_user do |f| %>
  <table>
    <tbody>
      <tr>
        <th>氏名</th>
        <th>（姓）</th>
        <th><%= f.text_filed :last_name %></th>
        <th>（名）</th>
        <th><%= f.text_filed :first_name %></th>
      </tr>
      <tr>
        <th>フリガナ</th>
        <th>（セイ）</th>
        <th><%= f.text_filed :kana_last_name %></th>
        <th>（メイ）</th>
        <th><%= f.text_filed :kana_first_name %></th>
      </tr>
      <tr>
        <th>郵便番号</th>
        <th><%= f.text_filed :post_number %></th>
      </tr>
      <tr>
        <th>住所</th>
        <th><%= f.text_filed :address %></th>
      </tr>
      <tr>
        <th>電話番号</th>
        <th><%= f.telephone_filed :telephone_number %></th>
      </tr>
      <tr>
        <th>メールアドレス<th>
        <th><%= f.email_filed :email %></th>
      </tr>
    </tbody>
  </table>

    <%= f.submit "編集内容を保存する", class: "btn btn-primary" %> / <%= link_to "退会する", '#' %>

<% end %>
