<div class="row">
	<div class="col-xs-10 col-xs-offset-1">
		<h4>会員一覧</h4>
		<table class="table">
		<thead>
				<tr>
					<th>会員ID</th>
					<th>氏名</th>
					<th>メールアドレス</th>
					<th>ステータス</th>
				</tr>
			</thead>

			<tbody>
				<% @end_users.each do |end_user| %>
					<tr>
						<td><%= end_user.id %></td>
							<td>
							<%= link_to owner_end_user_path(end_user) do %>
								<%= end_user.last_name %> <%= end_user.first_name %>
							<%end%>
							</td>
						<td><%= end_user.email %></td>
						<td><%= end_user.is_active ? '有効': '退会済' %></td>
					</tr>
				<%end%>
			</tbody>
		</table>
	</div>
</div>